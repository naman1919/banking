<div class="form-horizontal">
  <%= form_with url: managers_path do |form| %>
  <div class="form-group">
    <%= form.label "Enter Account Number:", class:"control-label" %>
    <%= form.text_field :account_number %>
  </div>

  <div class="form-group">
    <%= form.submit "Create Manager", class:"btn btn-info"%>
  </div>
</div>
<% end %>